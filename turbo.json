{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", "cdk.out/**"]
		},
		"deploy": {
			"dependsOn": ["build"]
		},
		"destroy": {},
		"preview": {
			"dependsOn": ["build"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
