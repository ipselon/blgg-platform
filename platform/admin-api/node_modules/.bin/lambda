#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/alex/Development/projects/ipselon/blgg-platform/node_modules/.pnpm/aws-lambda@1.0.7/node_modules/aws-lambda/bin/node_modules:/Users/alex/Development/projects/ipselon/blgg-platform/node_modules/.pnpm/aws-lambda@1.0.7/node_modules/aws-lambda/node_modules:/Users/alex/Development/projects/ipselon/blgg-platform/node_modules/.pnpm/aws-lambda@1.0.7/node_modules:/Users/alex/Development/projects/ipselon/blgg-platform/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/alex/Development/projects/ipselon/blgg-platform/node_modules/.pnpm/aws-lambda@1.0.7/node_modules/aws-lambda/bin/node_modules:/Users/alex/Development/projects/ipselon/blgg-platform/node_modules/.pnpm/aws-lambda@1.0.7/node_modules/aws-lambda/node_modules:/Users/alex/Development/projects/ipselon/blgg-platform/node_modules/.pnpm/aws-lambda@1.0.7/node_modules:/Users/alex/Development/projects/ipselon/blgg-platform/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../aws-lambda/bin/lambda" "$@"
else
  exec node  "$basedir/../aws-lambda/bin/lambda" "$@"
fi
